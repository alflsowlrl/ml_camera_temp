<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.tensorflow.lite.examples.detection">

    <uses-sdk />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher_facerec"
        android:roundIcon="@mipmap/ic_launcher_facerec_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.ObjectDetection">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/places_api_key"/>

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".CommuActivity"
            android:theme="@style/Theme.AppCompat"></activity>
        <activity android:name=".CapturedImageAcvtivity" />
        <activity
            android:name=".DetectorActivity"
            android:label="@string/tfe_od_app_name"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".RegisterActivity"></activity>

        <activity android:name=".Tab_Activity"></activity>
        <activity android:name=".Friend_gallery"></activity>
        <activity android:name=".tab1"></activity>
        <activity android:name=".tab2"></activity>
        <activity android:name=".tab3"></activity>

    </application>

</manifest>