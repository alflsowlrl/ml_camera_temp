<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.tensorflow.lite.examples.detection.MainActivity">

    <ImageView
        android:id="@+id/IUView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="-1dp"
        android:layout_toEndOf="@+id/bshView"
        android:src="@drawable/iu2" />

    <ImageView
        android:id="@+id/bshView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/cute"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="-3dp" />

    <Button
        android:id="@+id/RegisterBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bshView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="53dp"
        android:layout_marginTop="230dp"
        android:text="Register" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bshView"
        android:layout_marginStart="104dp"
        android:layout_marginTop="230dp"
        android:layout_toEndOf="@+id/RegisterBtn"
        android:text="Verify" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button2"
        android:layout_alignEnd="@+id/RegisterBtn"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="-121dp"
        android:text="TextView"
        android:textSize="30dp" />

    <ImageView
        android:id="@+id/realBshView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_below="@+id/bshView"
        android:layout_alignEnd="@+id/bshView"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="-91dp"
        android:src="@drawable/cute" />
</RelativeLayout>